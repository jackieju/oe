<h1>Listing guest_msgs</h1>

<table>
  <tr>
    <th>Uid</th>
    <th>Content</th>
  </tr>

<% @guest_msgs.each do |guest_msg| %>
  <tr>
    <td><%=h guest_msg.uid %></td>
    <td><%=h guest_msg.content %></td>
    <td><%= link_to 'Show', guest_msg %></td>
    <td><%= link_to 'Edit', edit_guest_msg_path(guest_msg) %></td>
    <td><%= link_to 'Destroy', guest_msg, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New guest_msg', new_guest_msg_path %>