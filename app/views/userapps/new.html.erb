<h1>New userapp</h1>

<% form_for(@userapp) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :uid %><br />
    <%= f.text_field :uid %>
  </p>
  <p>
    <%= f.label :appkey %><br />
    <%= f.text_field :appkey %>
  </p>
  <p>
    <%= f.label :permission %><br />
    <%= f.text_field :permission %>
  </p>
  <p>
    <%= f.label :style %><br />
    <%= f.text_field :style %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', userapps_path %>